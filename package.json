{
  "name": "krossykey",
  "version": "0.0.1",
  "author": "Afshawn Lotfi",
  "homepage": "http://krossykey.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build-browser": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "check": "gts check",
    "gtsclean": "gts clean",
    "compile": "tsc -p . && npm run genclean",
    "fix": "gts fix",
    "genclean": "find . -path ./node_modules -prune -o -path ./test-config -prune -o -type f -name '*.js' -exec rm {} \\; && find . -path ./node_modules -prune -o -path ./test-config -prune -o -type f -name '*.js.map' -exec rm {} \\;",
    "checkstyle": "(find ./src -path ./node_modules -prune -o -type f -name '*.html' -exec js-beautify -r --html -A force-aligned --editorconfig src/**/*.html {} \\;) && npm run compile && gts fix ",
    "posttest": "npm run check",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/cdk": "^6.3.0",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/material": "^6.3.0",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "4.7.0",
    "@ionic-native/secure-storage": "^4.8.0",
    "@ionic-native/splash-screen": "4.7.0",
    "@ionic-native/status-bar": "4.7.0",
    "@ionic/storage": "^2.1.3",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@types/cookie": "^0.3.1",
    "@types/downloadjs": "^1.4.0",
    "@types/object-hash": "^1.2.0",
    "@types/sprintf-js": "^1.1.0",
    "ajv": "^6.5.1",
    "clipboard-polyfill": "^2.4.7",
    "cookie": "^0.3.1",
    "cordova-browser": "5.0.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.2",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-secure-storage": "^2.6.8",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "crypto-js": "^3.1.9-1",
    "downloadjs": "^1.4.7",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "ng-circle-progress": "^1.0.0",
    "notp": "^2.0.3",
    "object-hash": "^1.3.0",
    "qrcode": "^1.2.0",
    "rxjs": "5.5.11",
    "sprintf-js": "^1.1.1",
    "sw-toolbox": "3.6.0",
    "thirty-two": "^1.0.2"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.10",
